<div id="wash"></div>
<div id="prompt_wrapper">
	<div id="prompt">
		<form id="login" action="/login" method="post">
			<div class="title">
				<h3>Please Login</h3>
			</div>
			<div class="body">
				<table class="form_table login">
					<tr>
						<td class="label">
							Username:
						</td>
						<td>
							<input type="text" name="username" />
						</td>
					</tr>
					<tr>
						<td class="label">
							Password:
						</td>
						<td>
							<input type="text" name="password" />
						</td>
					</tr>
					<tr>
						<td>
							&nbsp;
						</td>
						<td>
							<a href="/register">I don't have an account!</a>
						</td>
					</tr>
				</table>
			</div>
			<div class="buttons">
				<div class="contain">
					<input type="submit" value="Log In" />
					<a href="#" class="close_prompt">Cancel</a>
				</div>
			</div>
		</form>
	</div>
</div>